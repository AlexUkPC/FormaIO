<p id="notice"><%= notice %></p>

<h1>Answer Form Types</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Client form type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_form_types.each do |answer_form_type| %>
      <tr>
        <td><%= answer_form_type.id %></td>
        <td><%= answer_form_type.name %></td>
        <td><%= answer_form_type.client_form_type.name %></td>
        <td><%= link_to 'Show', answer_form_type %></td>
        <td><%= link_to 'Edit', edit_answer_form_type_path(answer_form_type) %></td>
        <td><%= link_to 'Destroy', answer_form_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer Form Type', new_answer_form_type_path %>
