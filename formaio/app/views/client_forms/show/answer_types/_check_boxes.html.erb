<% if @answer_form %>
  <%= collection_check_boxes( question.id,@answer_form.client_form.properties? ? @answer_form.client_form.properties[section.id.to_s][question.id.to_s].reject(&:empty?).map(&:to_i) : "", question.posible_answer, :id, :answer, checked: @answer_form.client_form.properties? ? (@answer_form.client_form.properties[section.id.to_s][question.id.to_s].reject(&:empty?).map(&:to_i)) : ""  )  do |b|
   b.label(:"data-value" => b.value) { b.check_box + b.text } end%>
<% else %>
  <%= collection_check_boxes( question.id,@client_form.properties? ? @client_form.properties[section.id.to_s][question.id.to_s].reject(&:empty?).map(&:to_i) : "", question.posible_answer, :id, :answer, checked: @client_form.properties? ? (@client_form.properties[section.id.to_s][question.id.to_s].reject(&:empty?).map(&:to_i)) : "" )  do |b|
   b.label(:"data-value" => b.value) { b.check_box + b.text } end%>
<% end %>